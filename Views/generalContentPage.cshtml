@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Pages>
@{
    Layout = "Layout.cshtml";

    if (Model.PageHeader != null)
    {
        @await Html.PartialAsync("Partials/ComponentsBuilder", Model.PageHeader)
    }

    if (Model.MainContent != null)
    {
        @await Html.PartialAsync("Partials/ComponentsBuilder", Model.MainContent)
    }
}